{
  "name": "iceman",
  "version": "0.0.0-development",
  "description": "Iceman sandboxes.",
  "scripts": {
    "dev": "ice start --speedup --mode development",
    "dev:secure": "ice start --speedup --https --mode development",
    "start": "ice start --speedup --mode production",
    "build": "ice build",
    "lint": "bun eslint && bun stylelint",
    "eslint": "eslint ./src --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,less}\" --cache",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "dependencies": {
    "@ice/runtime": "1.5.1",
    "@t3-oss/env-core": "0.11.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "2.6.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@applint/spec": "1.2.3",
    "@commitlint/cli": "19.6.1",
    "@ice/app": "3.5.1",
    "@ice/plugin-jsx-plus": "1.0.4",
    "@ice/plugin-store": "1.1.2",
    "@tailwindcss/postcss": "4.0.0",
    "@types/node": "22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "eslint": "8.35.0",
    "postcss": "8.5.1",
    "stylelint": "15.2.0",
    "tailwindcss": "4.0.0",
    "typescript": "5.7.3"
  },
  "trustedDependencies": [
    "@ice/app",
    "@ice/runtime",
    "@swc/core",
    "core-js",
    "core-js-pure",
    "esbuild"
  ]
}
